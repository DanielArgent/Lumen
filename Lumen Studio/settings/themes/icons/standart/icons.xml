      <Extension name=".py" icon="projectManager\python.png"/>
      <Extension name=".img|.png" icon="projectManager\image.png"/>
      <Extension name=".toml" icon="projectManager\toml.png"/>
    </Extensions>
  </ProjectManager>
</IconPack>