// Factorial function with simple recursion
let fact 0 = 1
let fact n = n * rec (n-1)

// Factorial with tail recursion and helper function
let fact' n =
    let fact'' 0 acc = acc
    let fact'' n acc = tailrec (n-1) (acc*n)
    fact'' n 1

// Factorial with ranges and reducing
let fact'' n =
    (1...n) / (_*_)

print (fact 5)    //-> 120
print (fact' 5)   //-> 120
print (fact'' 5)  //-> 120